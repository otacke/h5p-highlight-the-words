{
  "name": "h5p-highlight-the-words",
  "version": "0.1.4",
  "description": "Highlight words and sort them into the correct category",
  "private": true,
  "scripts": {
    "test": "jest --silent",
    "build": "cross-env NODE_ENV=production webpack",
    "watch": "webpack --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/stylesheets/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/stylesheets/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otacke/h5p-highlight-the-words.git"
  },
  "keywords": [
    "h5p",
    "library"
  ],
  "author": "Oliver Tacke",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-destructuring": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.5.0",
    "eslint-plugin-jest": "^28.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "regenerator-runtime": "^0.14.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "he": "^1.2.0"
  }
}
